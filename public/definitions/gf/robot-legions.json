  "specialRules": [
    {
      "name": "Darkness-Protocol",
      "description": "The hero and his unit get the Ambush special rule."
    },
    {
      "name": "Flux",
      "description": "For each unmodified result of 6 you roll to hit that hit is multiplied by 4."
    },
    {
      "name": "Gloom-Protocol",
      "description": "This model may block spells as if it had the Psychic(2) special rule. If it is a Psychic then it gets +2 to spell block rolls."
    },
    {
      "name": "Hunter",
      "description": "On a round in which this unit arrives from Ambush it gets AP(+3)."
    },
    {
      "name": "Regen-Protocol",
      "description": "The hero and his unit may ignore wounds from Regeneration on 4+."
    },
    {
      "name": "Royal March",
      "description": "The hero and his unit may ignore the Slow special rule."
    },
    {
      "name": "Shield Wall",
      "description": "Enemies get -1 to hit when they attack units where all models have this rule."
    },
    {
      "name": "Bot Fabricator",
      "description": "When this unit is activated place 1 Bot Swarm fully within 6” of this unit."
    },
    {
      "name": "Flux",
      "description": "For each unmodified result of 6 you roll to hit that hit is multiplied by 4."
    },
    {
      "name": "Gloom-Protocol",
      "description": "This model may block spells as if it had the Psychic(2) special rule."
    },
    {
      "name": "Psychic Master",
      "description": "This model counts as having the Psychic(3) rule, and may cast or block up to three spells each round, however for every unmodified result of 1 it takes 3 wounds."
    },
    {
      "name": "Psychic Warden",
      "description": "Whenever a model within 6” would take wounds from Psychic Master, you may put those wounds on this model instead."
    },
    {
      "name": "Reanimator",
      "description": "When this unit is activated pick one friendly unit within 12”, which may ignore wounds from Regeneration on 4+ next time it takes wounds."
    },
    {
      "name": "Repair",
      "description": "Once per turn, if within 2” of a unit with Tough, roll one die. On a 2+ you may repair 1 wound from the target."
    },
    {
      "name": "Royal March Order",
      "description": "When this model is activated pick one friendly unit within 12”, which may ignore the Slow special rule next time it moves."
    }
  ],
  "spells": [
    {
      "name": "Star Bots",
      "test": "4+",
      "description": "Target enemy unit within 12” gets -1 to melee next time it fights."
    },
    {
      "name": "Meteor Bots",
      "test": "4+",
      "description": "Target enemy unit within 12” takes 2 automatic hits with AP(2)."
    },
    {
      "name": "Assault Bots",
      "test": "5+",
      "description": "Target friendly unit within 12” may immediately move by up to 3”."
    },
    {
      "name": "Thunderbolt Bots",
      "test": "5+",
      "description": "Target enemy model within 12” takes 4 automatic hits."
    },
    {
      "name": "Arrow Bots",
      "test": "6+",
      "description": "Target enemy unit within 18” gets -2 to hit next time it shoots."
    },
    {
      "name": "Fire Bots",
      "test": "6+",
      "description": "Target 2 enemy units within 6” take 6 automatic hits with AP(1) each."
    }
  ]
